function [out] = scrabble_prototype(in)
    img = imread(in);
    img = imresize(img, [528 320]);
    img = rgb2gray(img);
    level = multithresh(img, 15);
    
    thresholded_img = imquantize(img, level);
    
    
    %Letters that were just played
    %imshow(thresholded_img, [15,16]);
    new_letters = thresholded_img == 15 | thresholded_img == 16;
    
    cc_new_letters = bwconncomp(new_letters);
    reg_new_letters = regionprops(cc_new_letters, 'Area','PixelIdxList' );
    
    
    idx = find(reg_new_letters.Area <=9
    for i = 1:cc_new_letters.NumObjects
        if reg_new_letters(i).Area <= 80 & reg_new_letters(i).Area>=30
            new_letters(reg_new_letters(i).PixelIdxList) = 1;
        else
            new_letters(reg_new_letters(i).PixelIdxList) = 0;
        
        end
    %imshow(new_letters)
    
    %Letters that were already on the board
    
    %imshow(thresholded_img, [1,4]);
    
    old_letters = thresholded_img < 4;
    
    all_letters = new_letters | old_letters;
    
    %imshow(all_letters)

    %imshow(old_letters)
    
    %Boundary that defines the board
    %imshow(thresholded_img, [10,11]);
    
    boundary = thresholded_img == 10 | thresholded_img == 12;
    
    imshow(boundary)
    
    
    
    
    lowest = bwlabel(thresholded_img == min(thresholded_img(:))); 
    middle = bwlabel(thresholded_img ~= min(thresholded_img(:)) & thresholded_img ~=max(thresholded_img(:)));
    highest = bwlabel(thresholded_img == max(thresholded_img(:)));
    
    %imshow(boundary);
    
    cc_lowest = bwconncomp(lowest);
    cc_middle = bwconncomp(middle);
    cc_highest = bwconncomp(highest);
    
    R_lowest = regionprops(lowest,'Area', 'Perimeter', 'PixelIdxList');
    R_middle = regionprops(middle,'Area', 'Perimeter', 'PixelIdxList');
    R_highest = regionprops(highest, 'Area', 'Perimeter', 'PixelIdxList');
    
    
    
    
    
end